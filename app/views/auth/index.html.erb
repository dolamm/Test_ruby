<h1>Login</h1>

<%= form_with url: login_path, local: true, method: :post do %>
<div>
    <%= label_tag= "email" %>
    <%= email_field_tag "email", nil %>

    <%= label_tag= "password" %>
    <%= password_field_tag "password", nil %>

    <%= submit_tag "Login" %>
</div>    

<% end %>

<%= link_to 'Register', signup_url %>